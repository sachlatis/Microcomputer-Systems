PART_A:
	IN 10H 
	LXI H,0900H 
	MVI A,FFH

SAVE_IN_MEM:
	MOV M,A
	DCR A
	CPI 00H
	JZ PART_B
	INX H
	JMP SAVE_IN_MEM

PART_B:
	LXI H,0900H
	MVI D,00H
	MVI E,00H
	MVI C,08H

LOAD_PART_B:
	MOV A,M

LOOP_PART_B:
	MOV B,A
	MOV A,C
	CPI 00H
	JZ CHECK_NEXT
	MOV A,B
	RAL
	DCR C
	JC LOOP_PART_B
	INX D
	JMP LOOP_PART_B

CHECK_NEXT:
	INX H
	MOV A,H
	CPI 0AH
	JZ PART_C
	MVI C,08H
	JMP LOAD_PART_B

PART_C:
	LXI H,0900H
	MVI C,00H

IN_RANGE:
	MOV A,M
	CPI 20H
	JC LOOK_NEXT
	CPI 71H
	JNC LOOK_NEXT
	INR C

LOOK_NEXT:
	INX H
	MOV A,H
	CPI 0AH
	JZ TO_END
	JMP IN_RANGE

TO_END:
	END